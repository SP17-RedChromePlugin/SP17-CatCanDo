{
    "manifest_version": 3,
    "name": "Don Cheadle Code",
    "description": "Practice for creating chrome extensions",
    "version": "1.0",
    "action": {
      "default_popup": "hello.html",
      "default_icon": "doncheadlehell.png"
    },
    "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://developer.chrome.com/docs/extensions/*",
        "https://developer.chrome.com/docs/webstore/*"
      ]
    }
  ]
}
  