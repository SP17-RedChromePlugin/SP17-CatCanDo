{
    "manifest_version": 3,
    "name": "Test Extension",
    "description": "Extension for learning how to build extensions",
    "version": "1.0",
    "permissions": ["tabs", "activeTab", "scripting"],
    "action": {
      "default_icon": "logo.png"
    },

    "background": {
    "service_worker": "background.js"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["addDiv.js"]
    }
    ],
    "web_accessible_resources": [
      {
        "resources": ["overlay.html", "catsitting.png"],
        "matches": ["<all_urls>"]
      }
    ]
}